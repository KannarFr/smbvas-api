GET     /resources                  controllers.ResourceController.getResources
GET     /resources/:uuid            controllers.ResourceController.getResourceById(uuid: String)
POST    /resources                  controllers.ResourceController.create
PATCH   /resources/:uuid            controllers.ResourceController.patchResourceById(uuid: String)
POST    /resources/:uuid/upload     controllers.ResourceController.uploadResourceContentToResourceId(uuid: String)

GET     /users                      controllers.UserController.getUsers
GET     /users/:uuid                controllers.UserController.getUserById(uuid: String)
POST    /users                      controllers.UserController.create
PATCH   /users/:uuid                controllers.UserController.patchUserById(uuid: String)
